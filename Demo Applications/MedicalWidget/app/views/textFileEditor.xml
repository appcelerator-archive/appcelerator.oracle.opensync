<Alloy>
	<Window class="container" onClose="onClose">
		<ScrollView id="editScroll">
			<TextArea id="editTextArea" onKeypressed="onKeyPressed"/>
		</ScrollView>
		<View id="buttonsRow">
			<Button id="cancelButton" onClick="doCancel"/>
			<Button id="saveButton" onClick="doSave"/>
		</View>
	</Window>
</Alloy>